<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>

<VBox
    xmlns="http://javafx.com/javafx/13" 
    xmlns:fx="http://javafx.com/fxml/1"
    fx:id="vboxLayout"
    fx:controller="chalmers.dat055.achtung.demo.FXMLTestController"
    alignment="center"
    minHeight="Infinity"
    minWidth="Infinity"
    stylesheets="@fxmlapp_main.css">

    <Label
        fx:id="helloLabel"        
        text="Hello"
        styleClass="menuitem"/>

    <Label
        fx:id="worldLabel"
        text="World"
        styleClass="menuitem"/>

    <Label
        fx:id="markLabel"
        text="!"
        styleClass="menuitem"/>

</VBox>
